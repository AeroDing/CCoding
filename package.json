{
  "publisher": "AeroDing",
  "name": "CCoding",
  "displayName": "CCoding",
  "version": "0.2.3",
  "description": "A comprehensive coding assistant with function lists, bookmarks, quick navigation, symbol pinning, todo lists, timeline, and keyword search for development",
  "author": "Pxin Ding",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/coding-helper"
  },
  "keywords": [
    "coding",
    "CCoding",
    "vue",
    "react",
    "function",
    "bookmark"
  ],
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onCommand:CCoding.showFunctionList",
    "onCommand:CCoding.addBookmark",
    "onCommand:CCoding.quickJump"
  ],
  "contributes": {
    "commands": [
      {
        "command": "CCoding.showFunctionList",
        "title": "Show Function List",
        "category": "CCoding"
      },
      {
        "command": "CCoding.addBookmark",
        "title": "Add Bookmark",
        "category": "CCoding"
      },
      {
        "command": "CCoding.showBookmarks",
        "title": "Show Bookmarks",
        "category": "CCoding"
      },
      {
        "command": "CCoding.quickJump",
        "title": "Quick Jump",
        "category": "CCoding"
      },
      {
        "command": "CCoding.pinSymbol",
        "title": "Pin Symbol",
        "category": "CCoding"
      },
      {
        "command": "CCoding.showTodos",
        "title": "Show Todo List",
        "category": "CCoding"
      },
      {
        "command": "CCoding.showTimeline",
        "title": "Show Timeline",
        "category": "CCoding"
      },
      {
        "command": "CCoding.searchKeywords",
        "title": "Search Keywords",
        "category": "CCoding"
      },
      {
        "command": "CCoding.addBookmarkFromContext",
        "title": "Add to Bookmarks",
        "category": "CCoding"
      },
      {
        "command": "CCoding.editBookmark",
        "title": "Edit Bookmark",
        "category": "CCoding"
      },
      {
        "command": "CCoding.removeBookmark",
        "title": "Remove Bookmark",
        "category": "CCoding"
      },
      {
        "command": "CCoding.addBookmarkFromEditor",
        "title": "Ê∑ªÂä†‰π¶Á≠æ",
        "category": "CCoding"
      },
      {
        "command": "CCoding.pinSymbolFromEditor",
        "title": "ÁΩÆÈ°∂Á¨¶Âè∑",
        "category": "CCoding"
      },
      {
        "command": "CCoding.unpinSymbol",
        "title": "ÁßªÈô§ÁΩÆÈ°∂",
        "category": "CCoding"
      },
      {
        "command": "CCoding.clearAllPinnedSymbols",
        "title": "Ê∏ÖÁ©∫ÊâÄÊúâÁΩÆÈ°∂Á¨¶Âè∑",
        "category": "CCoding",
        "icon": "$(clear-all)"
      },
      {
        "command": "CCoding.repairData",
        "title": "‰øÆÂ§çÊï∞ÊçÆ",
        "category": "CCoding"
      }
    ],
    "keybindings": [
      {
        "command": "CCoding.showFunctionList",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f"
      },
      {
        "command": "CCoding.addBookmark",
        "key": "ctrl+shift+b",
        "mac": "cmd+shift+b"
      },
      {
        "command": "CCoding.quickJump",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+j"
      }
    ],
    "views": {
      "CCoding": [
        {
          "id": "CCoding.tabSwitcher",
          "name": "ÊéßÂà∂Èù¢Êùø",
          "type": "webview",
          "initialSize": 150,
          "when": "true"
        },
        {
          "id": "CCoding.functionList",
          "name": "Á¨¶Âè∑ÂàóË°®",
          "when": "CCoding.currentTab == 'symbols' && (resourceExtname == .js || resourceExtname == .ts || resourceExtname == .vue || resourceExtname == .jsx || resourceExtname == .tsx || resourceExtname == .html || resourceExtname == .css || resourceExtname == .scss || resourceExtname == .less)",
          "visibility": "visible"
        },
        {
          "id": "CCoding.bookmarks",
          "name": "‰π¶Á≠æ",
          "when": "CCoding.currentTab == 'current' || CCoding.currentTab == 'all'",
          "visibility": "visible"
        },
        {
          "id": "CCoding.todos",
          "name": "ÂæÖÂäû‰∫ãÈ°π",
          "when": "CCoding.currentTab == 'current' || CCoding.currentTab == 'all'",
          "visibility": "visible"
        },
        {
          "id": "CCoding.pinnedSymbols",
          "name": "ÁΩÆÈ°∂Á¨¶Âè∑",
          "when": "CCoding.currentTab == 'current' || CCoding.currentTab == 'all'",
          "visibility": "visible"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "CCoding",
          "title": "CCoding",
          "icon": "src/images/logo.svg"
        }
      ]
    },
    "menus": {
      "explorer/context": [
        {
          "command": "CCoding.addBookmarkFromContext",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "submenu": "CCoding.editorSubmenu",
          "when": "editorTextFocus",
          "group": "navigation@1"
        }
      ],
      "CCoding.editorSubmenu": [
        {
          "command": "CCoding.addBookmarkFromEditor",
          "when": "editorTextFocus"
        },
        {
          "command": "CCoding.pinSymbolFromEditor",
          "when": "editorTextFocus"
        }
      ],
      "view/item/context": [
        {
          "command": "CCoding.editBookmark",
          "when": "view == CCoding.bookmarks && viewItem == bookmark",
          "group": "inline"
        },
        {
          "command": "CCoding.removeBookmark",
          "when": "view == CCoding.bookmarks && viewItem == bookmark",
          "group": "inline"
        },
        {
          "command": "CCoding.unpinSymbol",
          "when": "view == CCoding.pinnedSymbols && viewItem == pinnedSymbol",
          "group": "inline"
        }
      ],
      "view/title": [
        {
          "command": "CCoding.clearAllPinnedSymbols",
          "when": "view == CCoding.pinnedSymbols && CCoding.hasPinnedSymbols",
          "group": "navigation"
        }
      ]
    },
    "submenus": [
      {
        "id": "CCoding.editorSubmenu",
        "label": "CCoding"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run lint && npm run compile",
    "compile": "node scripts/build.js",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "build": "npm run lint && npm run compile && npm run package",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "bumpp": "bumpp",
    "publish": "vsce publish",
    "postbumpp": "npm run compile && npx vsce package --out CCoding-$(node -p \"require('./package.json').version\").vsix && echo 'üéâ ÁâàÊú¨ÂèëÂ∏ÉÂÆåÊàêÔºÅ' && echo 'üì¶ Êú¨Âú∞ÁîüÊàêÊñá‰ª∂: CCoding-$(node -p \"require('./package.json').version\").vsix' && echo 'üöÄ GitHub Actions Â∞ÜËá™Âä®ÂàõÂª∫ Release Âπ∂‰∏ä‰º†Êñá‰ª∂'"
  },
  "dependencies": {
    "bumpp": "^10.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.15.0",
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "glob": "^11.0.3",
    "typescript": "^4.9.4",
    "vsce": "^2.15.0"
  }
}
